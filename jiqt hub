local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Jiq Hub (made by jiqta)", HidePremium = false, SaveConfig = true, ConfigFolder = "Jiqta"})

OrionLib:MakeNotification({
	Name = "Notification!",
	Content = "Welcome to jiq hub! if some do not work i will update them. NOTE: this is a new script hub and i am adding new scripts every day!",
	Image = "rbxassetid://4483345998",
	Time = 5
})

setclipboard("https://discord.gg/usZVsbGe")
toclipboard("https://discord.gg/usZVsbGe")

--[[
Title = <string> - The title of the notification.
Content = <string> - The content of the notification.
Image = <string> - The icon of the notification.
Time = <number> - The duration of the notfication.
]]

local Tab = Window:MakeTab({
	Name = "Local Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Local player"
})
Tab:AddSlider({
	Name = "Walk speed",
	Min = 16,
	Max = 500,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walk speed",
	Callback = function(v) -- 500 (MaxValue) | 0 (MinValue)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
	end    
})

Tab:AddSlider({
	Name = "Jump Power",
	Min = 50,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump Power",
	Callback = function(v) -- 500 (MaxValue) | 0 (MinValue)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
	end    
})

